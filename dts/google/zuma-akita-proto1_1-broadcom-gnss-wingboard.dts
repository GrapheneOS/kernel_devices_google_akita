/dts-v1/;
/plugin/;

#include "zuma-akita-common.dtsi"
#include "../google-dts-base/zuma-board-gps.dtsi"

/ {
	board_id = <0x40402>;
	board_rev = <0x010102>;
	fragment@boardbase {
		target-path = "/";
		__overlay__ {
			model = "ZUMA AKITA Proto 1.1 Broadcom GNSS Wingboard based on ZUMA";
			compatible = "google,ZUMA AKITA", "google,ZUMA";
		};
	};
};
